<div class="container">
  <h2>List Post</h2>
  <table class="table">
    <thead>
      <tr>
        <th>id</th>
        <th>Title</th>
        <th>Tags</th>
      </tr>
    </thead>
    <tbody>
      <% @posts.each do |post| %>
        <%= render post %>
      <% end %>
    </tbody>
  </table>
  <%= will_paginate @posts %>
</div>
